    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Test Flask-Plugin",
            "type": "python",
            "request": "launch",
            "module": "flask_plugin.test.run",
            "args": ["--level=2"]
        },
        {
            "name": "Python: Debug Flask-Plugin",
            "type": "python",
            "request": "launch",
            "program": "example/app.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}