    "version": "0.2.0",
    "configurations": [
        {
            "name": "Install Flask-Plugin",
            "type": "python",
            "request": "launch",
            "program": "setup.py",
            "console": "integratedTerminal",
            "args": ["install", "--install-lib", "/Users/doge/Library/Python/3.8/lib/python/site-packages"]
        },
        {
            "name": "Package Flask-Plugin",
            "type": "python",
            "request": "launch",
            "program": "setup.py",
            "console": "integratedTerminal",
            "args": [
                "clean",
                "--all",
                "build"
            ]
        },
        {
            "name": "Test Flask-Plugin",
            "type": "python",
            "request": "launch",
            "module": "tests.run",
            "args": [
                "--level=2"
            ]
        },
        {
            "name": "Develop Flask-Plugin",
            "type": "python",
            "request": "launch",
            "program": "example/app.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}